        android:icon="@mipmap/tictactoe_icon_img"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tictactoe_icon_img"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".ThreeMultiActivity" />
        <activity android:name=".FiveMultiActivity" />
        <activity android:name=".ThreeSingleActivity" />
        <activity android:name=".ScoreBoardActivity" />
        <activity android:name=".FiveSingleActivity" />
        <activity android:name=".About" />
    </application>

</manifest>